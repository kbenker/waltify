{
	"manifest_version": 2,

	"name": "Waltify",
  "short_name": "Waltify",
	"description": "Walt likes snakes.",
	"author": "Matthew Brennan",
	"version": "0.9",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

